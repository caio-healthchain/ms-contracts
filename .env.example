# Database PostgreSQL (Write Model)
# Substitua pelas suas credenciais
DATABASE_URL="postgresql://usuario:senha@host:5432/database?sslmode=require"

# Cosmos DB / MongoDB (Read Model)
# Substitua pelas suas credenciais
COSMOSDB_URI="mongodb+srv://usuario:senha@host:10255/?ssl=true&replicaSet=globaldb&retrywrites=false&maxIdleTimeMS=120000"
COSMOSDB_DATABASE="lazarus"

# Server Configuration
PORT=3001
NODE_ENV=development

# Logs
LOG_LEVEL=debug

# CORS (opcional)
CORS_ORIGIN=http://localhost:3000

# JWT (se implementar autenticação)
# JWT_SECRET=seu_secret_aqui
# JWT_EXPIRES_IN=24h
